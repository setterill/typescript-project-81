{
    "compilerOptions": {
        "target": "ESNext",
        "skipLibCheck": true,
        "noImplicitAny": true,
        "checkJs": true,
        "strict": true,
        "noEmit": false,
        "outDir": "./dist",
        "moduleResolution": "bundler",
        "module": "preserve",
        "esModuleInterop": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false
    },
    "include": [
        "/**/*",
    ],
}